# ls
alias ls='ls --color=auto'
alias ll='ls --color=auto -hl'
alias la='ls --color=auto -hlA'

# grep
alias grep='grep --color=auto'

# git
alias gs='git status'
alias gl='git log'
alias gc='git commit'
alias gb='git branch'

# dotfiles
alias dotfiles='/usr/bin/git --git-dir=$HOME/.dotfiles/ --work-tree=$HOME'
